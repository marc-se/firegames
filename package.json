{
  "name": "gamesdb",
  "version": "0.2.0",
  "description": "A tool to organize your games collection",
  "main": "src/index.js",
  "scripts": {
    "start": "node ./build/server/server.js",
    "build": "rimraf ./build && npm run build-app && npm run build-server",
    "build-app": "webpack --config webpack.config",
    "production": "NODE_ENV=production webpack --config webpack.config",
    "build-server": "webpack --config webpack.server.config",
    "build-server-babel": "babel src/server --out-dir build/server",
    "dev": "babel-node ./src/server/server.js",
    "lint": "eslint src/**/**",
    "docker-dev": "yarn install && nodemon --watch package.json --exec \"yarn install && npm run dev\""
  },
  "author": "Marcel Sebald",
  "dependencies": {
    "antd": "^2.9.1",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "classnames": "^2.2.5",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "firebase": "^3.4.0",
    "less": "^2.7.2",
    "less-loader": "^3.0.0",
    "raw-loader": "^0.5.1",
    "react": "^15.1.0",
    "react-countup": "^1.3.0",
    "react-dom": "^15.1.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.0",
    "react-svg-inline": "^2.0.0",
    "redux": "^3.6.0",
    "webpack": "^1.13.2"
  },
  "license": "UNLICENSED",
  "repository": "",
  "devDependencies": {
    "@sep/eslint-config-sep-react": "^1.0.12",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.12.0",
